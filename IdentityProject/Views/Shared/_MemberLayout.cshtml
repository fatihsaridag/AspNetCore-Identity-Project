<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Identity Project </title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/feather/feather.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/ti-icons/css/themify-icons.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/typicons/typicons.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/simple-line-icons/css/simple-line-icons.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/css/vendor.bundle.base.css">
  <!-- endinject -->
  <!-- Plugin css for this page -->
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
  <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/js/select.dataTables.min.css">
  <!-- End plugin css for this page -->
  <!-- inject:css -->
    <link rel="stylesheet" href="~/star-admin2-free-admin-template-1.0.0/template/css/vertical-layout-light/style.css">
  <!-- endinject -->
    <link rel="shortcut icon" href="~/star-admin2-free-admin-template-1.0.0/template/images/favicon.png" />
</head>
<body>
  <div class="container-scroller"> 
    <!-- partial:partials/_navbar.html -->
    <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex align-items-top flex-row">
      <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-start">
        <div class="me-3">
          <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-bs-toggle="minimize">
            <span class="icon-menu"></span>
          </button>
        </div>
        <div>
                    <h2> IDENTITY PROJECT</h2>
                </div>
      </div>
      <div class="navbar-menu-wrapper d-flex align-items-top"> 
        <ul class="navbar-nav">
          <li class="nav-item font-weight-semibold d-none d-lg-block ms-0">
            <h1 class="welcome-text"> Member Anasayfaya Hoş geldiniz. </h1>
          </li>
                  

        </ul>

        <ul class="navbar-nav text-center ml-3">
              <li class="nav-item mr-4 ml-3">
                        <a asp-controller="Admin" asp-action="Index" class="btn btn-outline-primary">Admin</a>
                    </li>

                    <li class="nav-item mr-4">
                        <a asp-controller="Member" asp-action="Editor" class="btn btn-outline-info">Editor </a>
                    </li>

                    <li class="nav-item mr-4">
                        <a  asp-controller="Member" asp-action="Manager" class="btn btn-outline-success">Manager </a>
                    </li>

                    <li class="nav-item mr-4">
                        <a asp-controller="Member" asp-action="Eskisehir" class="btn btn-outline-danger">Eskişehir </a>
                    </li>
                    <li class="nav-item mr-4">
                        <a asp-controller="Member" asp-action="LicensePolicy" class="btn btn-outline-warning">Ehliyet </a>
                    </li>
        </ul>

        <ul class="navbar-nav ms-auto ">

          <li class="nav-item dropdown d-none d-lg-block user-dropdown">
            <a class="nav-link" id="UserDropdown" href="#" data-bs-toggle="dropdown" aria-expanded="false">
                            <h6>Kullanıcı : @User.Identity.Name</h6>
                            <img class="img-xs rounded-circle" src="~/star-admin2-free-admin-template-1.0.0/template/images/faces/face8.jpg" alt="Profile image">
                        </a>
            <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="UserDropdown">
              <div class="dropdown-header text-center">
                                <img class="img-md rounded-circle" src="~/star-admin2-free-admin-template-1.0.0/template/images/faces/face8.jpg" alt="Profile image">
                <p class="mb-1 mt-3 font-weight-semibold">Fatih Sarıdağ</p>
                <p class="fw-light text-muted mb-0">fatihsaridag26@gmail.com</p>
              </div>
              <a class="dropdown-item"><i class="dropdown-item-icon mdi mdi-account-outline text-primary me-2"></i> Profilim </a>
              <a asp-controller="Member" asp-action="UserEdit"  class="dropdown-item"><i class="dropdown-item-icon mdi mdi-message-text-outline text-primary me-2"></i> Kullanıcı Düzenle </a>
              <a asp-controller="Home" asp-action="ResetPassword" class="dropdown-item"><i class="dropdown-item-icon mdi mdi-calendar-check-outline text-primary me-2"></i> Şifremi Unuttum</a>
              <a asp-controller="Member" asp-action="LogOut" asp-route-returnUrl="/Home/Index" class="dropdown-item"><i class="dropdown-item-icon mdi mdi-power text-primary me-2"></i>Çıkış Yap</a>
            </div>
          </li>
        </ul>
        <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-bs-toggle="offcanvas">
          <span class="mdi mdi-menu"></span>
        </button>
      </div>
    </nav>
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_settings-panel.html -->
      
    @RenderBody()
    </div>


  </div>
  <!-- container-scroller -->




  <!-- plugins:js -->
    <script src="~/star-admin2-free-admin-template-1.0.0/template/vendors/js/vendor.bundle.base.js"></script>
  <!-- endinject -->
  <!-- Plugin js for this page -->
  <script src="~/star-admin2-free-admin-template-1.0.0/template/vendors/chart.js/Chart.min.js"></script>
  <script src="~/star-admin2-free-admin-template-1.0.0/template/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
  <script src="~/star-admin2-free-admin-template-1.0.0/template/vendors/progressbar.js/progressbar.min.js"></script>

  <!-- End plugin js for this page -->
  <!-- inject:js -->
  <script src="~/star-admin2-free-admin-template-1.0.0/template/js/off-canvas.js"></script>
  <script src="~/star-admin2-free-admin-template-1.0.0/template/js/hoverable-collapse.js"></script>
  <script src="~/star-admin2-free-admin-template-1.0.0/template/js/template.js"></script>
  <script src="~/star-admin2-free-admin-template-1.0.0/template/js/settings.js"></script>
    <script src="~/star-admin2-free-admin-template-1.0.0/template/js/todolist.js"></script>
  <!-- endinject -->
  <!-- Custom js for this page-->
  <script src="js/dashboard.js"></script>
  <script src="js/Chart.roundedBarCharts.js"></script>
  <!-- End custom js for this page-->
</body>

</html>

